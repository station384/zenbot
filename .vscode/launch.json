    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Zenbot Backfill",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/zenbot.js",
            "cwd": "${workspaceFolder}",
            "args": [ "backfill", "bittrex.ada-btc"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Zenbot Paper Trading",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/zenbot.js",
            "cwd": "${workspaceFolder}",
            "args": [ "trade", "binance.bnb-usdt", "--paper", "--period=1m"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Zenbot Sim srsi_bollinger",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/zenbot.js",
            "cwd": "${workspaceFolder}",
            "args": [ "sim", "binance.bnb-usdt",  "--period=1m", "--strategy=srsi_bollinger","--order_type=maker"]
        },        {
            "type": "node",
            "request": "launch",
            "name": "Launch Zenbot Sim srsi_neural",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/zenbot.js",
            "cwd": "${workspaceFolder}",
            "args": [ "sim", "binance.bnb-usdt",  "--period=5m", "--strategy=srsi_neural","--order_type=maker"]
        },   
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Zenbot Sim wavetrend",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/zenbot.js",
            "cwd": "${workspaceFolder}",
            "args": [ "sim", "binance.bnb-usdt",  "--period=1m", "--strategy=wavetrend","--order_type=maker"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Update Markets Bittrex",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/extensions/exchanges/bittrex/update-products.js",
            "cwd": "${workspaceFolder}/extensions/exchanges/bittrex"

        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Genetic Backtester",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/scripts/genetic_backtester/darwin.js",
            "cwd": "${workspaceFolder}",
            "args": [
                "--selector=binance.bnb-usdt",
                "--asset_capital=0.0",
                "--currency_capital=100.0",
                "--use_strategies=srsi_bollinger",
                //trend_bollinger,trend_ema,
                "--population=100",
                "--population_data=",
                "--days=15",
                "--order_type=maker",
                "--markup_sell_pct=0.9",
                "--markdown_buy_pct=0.9",
                "--sell_stop_pct=3",
                "--buy_stop_pct=8",
                "--period_length=10m",


                  "--maxCores=7",  
                  "--generateLaunch", 
                  "--noStatSave", 
                  "--ignoreLaunchFitness", 
                  "--overbought_rsi=70", 
                  "--oversold_rsi=30",  
                  "--min_periods=25", 
                  "--minTrades=2"

                ]
        },
        
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Zenbot !!!Trade Live Bittrex!!!",
            "console": "externalTerminal",
            "program": "${workspaceFolder}/zenbot.js",
            "cwd": "${workspaceFolder}",
            "args": [ "trade", "bittrex.XRP-USDT", 
                "--avg_slippage_pct=0.045", 
                "--backtester_generation=37",
                "--bollinger_lower_bound_pct=16.573725742046623",
                "--bollinger_size=5",
            "--bollinger_time=5.718442655882575",
            "--bollinger_upper_bound_pct=6.55491217292086",
"--buy_stop_pct=12",
"--exchangeMarketPair=bittrex.xrp-usdt",
"--markdown_buy_pct=0.6928484993307022",
"--markup_sell_pct=-0.0014423681389312826",
"--max_sell_loss_pct=25",
"--max_slippage_pct=5",
"--min_periods=20",
"--order_type=maker",
"--period=16m",
"--period_length=16m",
"--profit_stop_enable_pct=0",
"--profit_stop_pct=9",
"--rsi_periods=14",
"--sell_stop_pct=36",
"--strategy=noop"
            ]
        }
    ]
}